{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "yoda": [ 2 ],
    "indent": [ 2, "tab", { "SwitchCase":  2 } ],
    "linebreak-style": [ 2, "unix" ],
    "quotes": [ 2, "single" ],
    "semi": [ 2, "always" ],
    "no-console": [ 1 ],
    "no-alert": [ 1 ],
    "camelcase": [ 2 ],
    "react/prop-types": 0
  },
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "requireConfigFile": false
  },
  "parser": "@babel/eslint-parser",
  "extends": ["prettier"],
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}